{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Remote Debug",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/build/zkProver",
          "args": ["-c", "/root/zkevm-prover/poc_config/config_generate_input_file.json"],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
        "miDebuggerPath": "/usr/bin/gdb"
      }
  ]
}
